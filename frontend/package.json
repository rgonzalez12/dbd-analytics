{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && tsc -p . --noEmit",
		"prepare": "svelte-kit sync || echo ''",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
